{
  "name": "aprenda-excel-usando-excel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bg": "node ./src/styles/base64-bg.js",
    "bg:watch": "node ./src/styles/base64-bg.js --watch",
    "dev": "concurrently -k --names \"base64,next.js\" -c \"bgCyan.bold.white,gray\" \"npm run bg:watch\" \"next dev\"",
    "build": "npm run bg && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "args": "^5.0.3",
    "concurrently": "^8.2.1",
    "css-b64-images": "^0.2.5",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "modern-normalize": "^2.0.0",
    "next": "^13.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
